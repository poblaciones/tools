
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="medea.entities" assembly="medea.data">
	<class name="Contact" table="contact">
		<id name="Id" column="con_id" unsaved-value="null">
			<generator class="assigned" />
		</id>
		<property name="Email" column="con_email" not-null="false" length="50" />
		<property name="Person" column="con_person" not-null="false" length="255" />
		<property name="Phone" column="con_phone" not-null="false" length="50" />
	</class>
</hibernate-mapping>
